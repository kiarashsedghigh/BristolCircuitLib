library(demo) {
  cell(BUF) {
    area: 6;
    pin(A) { direction: input; }
    pin(Z) { direction: output;
              function: "A"; }
  }
  cell(INV) {
    area: 3;
    pin(A) { direction: input; }
    pin(Z) { direction: output;
              function: "A'"; }
  }
  
  cell(AND2) {
    area: 4;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Z) { direction: output;
             function: "A*B"; }
  }

  cell(XOR2) {
    area: 4;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Z) { direction: output;
             function: "A^B"; }
  }
}